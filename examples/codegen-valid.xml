<?xml version="1.0"?>
<!DOCTYPE class SYSTEM "class.dtd">
<class name="Employee" package="com.example.hr">
  <field name="employeeId" type="long"/>
  <field name="name" type="String"/>
  <field name="department" type="String"/>
  <field name="salary" type="double"/>
  <method name="getId" returnType="long">
    <body>return employeeId;</body>
  </method>
  <method name="getName" returnType="String">
    <body>return name;</body>
  </method>
  <method name="calculateBonus" returnType="double">
    <body>return salary * 0.1;</body>
  </method>
</class>
